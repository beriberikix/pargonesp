/ {
	aliases {
		en = &en_pin;
		boot = &boot_pin;
		uart = &uart1;
	};

	leds {
		compatible = "gpio-leds";
		en_pin: en_pin {
			gpios = <&gpio0 24 GPIO_ACTIVE_HIGH>;
		};
		boot_pin: boot_pin {
			gpios = <&gpio0 16 GPIO_ACTIVE_HIGH>;
		};
	};
};

&pinctrl {
	uart1_example: uart1_example {
		group1 {
			psels = <NRF_PSEL(UART_TX, 1, 5)>,
							<NRF_PSEL(UART_RX, 1, 4)>;
		};
	};
};

&uart1 {
	status = "okay";
	pinctrl-0 = <&uart1_example>;
};